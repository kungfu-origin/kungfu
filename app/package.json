{
  "name": "kungfu",
  "version": "2.3.0",
  "description": "KungFu Trader App",
  "license": "Apache-2.0",
  "homepage": "https://github.com/taurusai/kungfu",
  "author": {
    "name": "taurus.ai",
    "email": "info@taurus.ai"
  },
  "env": [
    "USE_HARD_LINKS=false"
  ],
  "config": {
    "pyver": "three",
    "cmakejsopt": ""
  },
  "main": "./dist/app/main.js",
  "engines": {
    "node": ">=10.16.0 <11.0.0"
  },
  "scripts": {
    "install-deps": "electron-builder install-app-deps",
    "clean": "rimraf ./dist/* ./build/*",
    "build:install": "electron-builder",
    "build": "node --max_old_space_size=8192 .electron-kungfu/build.js && electron-builder",
    "build-cli": "node --max_old_space_size=8192 .electron-kungfu/build.js && electron-builder --config.productName=Kungfu-CLI",
    "dev": "node .electron-kungfu/dev-runner.js",
    "dist": "yarn run dist:main && yarn run dist:render",
    "dist:main": "cross-env NODE_ENV=production node --max_old_space_size=8192 ../node_modules/webpack/bin/webpack.js --config .electron-kungfu/webpack.main.config.js",
    "dist:render": "cross-env NODE_ENV=production node --max_old_space_size=8192 ../node_modules/webpack/bin/webpack.js --config .electron-kungfu/webpack.renderer.config.js",
    "test": " mocha"
  },
  "build": {
    "productName": "Kungfu",
    "artifactName": "${productName}-${buildVersion}-${os}-${arch}-${channel}.${ext}",
    "generateUpdatesFilesForAllChannels": true,
    "appId": "taurus.ai.KungFu.Trader",
    "directories": {
      "output": "build/app"
    },
    "publish": [
      {
        "provider": "generic",
        "url": "http://taurusai.com/kungfu"
      }
    ],
    "files": [
      "dist/app/**/*",
      {
        "from": ".",
        "to": "dist/app",
        "filter": [
          "resources"
        ]
      },
      "!**/kungfu-core/*",
      "!**/kungfu-core/**/*",
      "!**/kungfu-core/**/**/*",
      "**/kungfu-core/build/*/libkungfu.*",
      "**/kungfu-core/build/*/kungfu.*",
      "**/kungfu-core/build/*/pykungfu*",
      "**/kungfu-core/build/*/node_kungfu.node",
      "**/kungfu-core/build/*/kungfubuildinfo.json",
      "**/kungfu-core/node/*",
      "**/kungfu-core/node/**/*",
      "**/kungfu-core/package.json"
    ],
    "extraResources": [
      {
        "from": "../core/build/kfc",
        "to": "kfc",
        "filter": [
          "!**/btdata"
        ]
      },
      {
        "from": "../core/build/python/dist",
        "to": "kungfu-resources/python",
        "filter": [
          "kungfu-*-py3-*.whl"
        ]
      },
      {
        "from": "./resources",
        "to": "kungfu-resources",
        "filter": [
          "config/*"
        ]
      },
      {
        "from": "../kfext_task_timerTrader/build/ext",
        "to": "kungfu-extensions/timer-trader"
      },
      {
        "from": "../cli2/build/cli",
        "to": "kungfu-cli",
        "filter": [
          "fsevents.node",
          "kungfu-cli",
          "kungfu-cli.*",
          "node_kungfu.node",
          "kungfu.dll",
          "kungfubuildinfo.json",
          "pykungfu*",
          "libkungfu.*"
        ]
      }
    ],
    "asar": false,
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "./resources/logo/icon.icns",
      "type": "distribution",
      "target": [
        "dmg"
      ]
    },
    "win": {
      "icon": "./resources/logo/icon.ico",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "linux": {
      "icon": "./resources/logo/icn.icns",
      "target": [
        "rpm",
        "appimage"
      ]
    },
    "nsis": {
      "oneClick": false,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "installerIcon": "./resources/logo/icon.ico",
      "uninstallerIcon": "./resources/logo/icon.ico",
      "installerHeaderIcon": "./resources/logo/icon.ico",
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true
    }
  },
  "dependencies": {
    "fkill": "^6.1.0",
    "fs-extra": "7.0.1",
    "gaze": "^1.1.3",
    "kungfu-core": "*",
    "pm2": "4.5.1"
  },
  "devDependencies": {
    "@types/node": "^12.6.2",
    "aws-sdk": "^2.395.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-component": "^1.1.1",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "babili-webpack-plugin": "^0.1.2",
    "bindings": "^1.5.0",
    "cfonts": "^1.1.3",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-roughly": "^1.0.0",
    "chalk": "^4.1.0",
    "colors": "^1.3.3",
    "compression-webpack-plugin": "^2.0.0",
    "copy-webpack-plugin": "^4.0.1",
    "copyfiles": "^2.1.0",
    "cross-env": "^5.0.5",
    "cross-var": "^1.1.0",
    "css-loader": "^2.1.1",
    "del": "^3.0.0",
    "devtron": "^1.4.0",
    "electron": "4.2.11",
    "electron-builder": "^22.3.2",
    "electron-chromedriver": "^5.0.1",
    "electron-debug": "^1.4.0",
    "electron-devtools-installer": "^2.2.0",
    "electron-updater": "^4.0.6",
    "element-ui": "2.8.2",
    "extract-text-webpack-plugin": "^3.0.0",
    "fast-csv": "^2.5.0",
    "file-loader": "^0.11.2",
    "getprocesses": "^1.1.1",
    "html-webpack-plugin": "^2.30.1",
    "inject-loader": "^3.0.0",
    "jquery": "^3.4.1",
    "log4js": "^4.0.2",
    "mime": "^2.4.0",
    "minimist": "^1.2.5",
    "mkdirp": "^0.5.1",
    "mocha": "^6.1.4",
    "moment": "^2.24.0",
    "monaco-editor": "^0.21.2",
    "monaco-editor-webpack-plugin": "^2.0.0",
    "multispinner": "^0.2.1",
    "node-loader": "^0.6.0",
    "node-sass": "^4.12.0",
    "optimize-js-plugin": "^0.0.4",
    "physical-cpu-count": "^2.0.0",
    "preload-webpack-plugin": "^2.3.0",
    "readline": "^1.3.0",
    "require-dir": "^1.0.0",
    "rimraf": "^2.6.3",
    "rxjs": "^6.5.2",
    "sass-loader": "^7.0.3",
    "spectron": "^7.0.0",
    "style-loader": "^0.21.0",
    "tail": "^2.0.2",
    "taskkill": "^2.0.0",
    "ts-loader": "3.5.0",
    "typescript": "^3.5.3",
    "uglifyjs-webpack-plugin": "2",
    "url-loader": "^1.1.2",
    "vue": "^2.6.11",
    "vue-electron": "^1.0.6",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^13.0.5",
    "vue-router": "^3.0.1",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.4.2",
    "vue-virtual-scroller": "^1.0.0-rc.2",
    "vuex": "^3.0.1",
    "webpack": "3.12.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-dev-server": "3.0.0",
    "webpack-hot-middleware": "^2.18.2",
    "webpack-merge": "^4.1.0",
    "webpack-permissions-plugin": "^1.0.0"
  }
}
